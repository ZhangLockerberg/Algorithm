   "source": [
    "nums = [3,2,6,4,7,1,22,45,0,11,32,23,2]\n",
    "\n",
    "def sort(nums):\n",
    "    n = len(nums)\n",
    "    for i in range(n):\n",
    "        for j in range(i+1,n):\n",
    "            if nums[i] > nums[j]:\n",
    "                nums[i],nums[j] = nums[j],nums[i]\n",
    "    return nums\n",
    "\n",
    "sort(nums)"
   ]
  }
